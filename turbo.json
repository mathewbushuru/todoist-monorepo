{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "clean"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "predeploy": {
      "dependsOn": ["build", "test"]
    },
    "clean": {
      "cache": false
    },
    "preview": {
      "dependsOn": ["build"]
    },
    "start": {
      "dependsOn": ["build"]
    },
    "tdd": {},
    "test": {
      "dependsOn": ["build"]
    }
  }
}
